<!-- Carousel Start -->
    <div class="container-fluid p-0 mb-5">
        <div id="header-carousel" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <% modeles.forEach( (element,i) => { 
                    if(i==0){%>
                    <div class="carousel-item active">
                        <img class="w-100" src="Vue/img/carousel-bg-1.jpg" alt="Image">
                        <div class="carousel-caption d-flex align-items-center">
                            <div class="container">
                                <div class="row align-items-center justify-content-center justify-content-lg-start">
                                    <div class="col-10 col-lg-7 text-center text-lg-start">
                                        <h6 class="text-white text-uppercase mb-3 animated slideInDown">// RAUDI //
                                        </h6>
                                        <h1 class="display-3 text-white mb-4 pb-3 animated slideInDown"><%= element.nom %></h1>
                                        <a href="modele/<%=element.id%>" class="btn btn-primary py-3 px-5 animated slideInDown">Voir plus<i
                                                class="fa fa-arrow-right ms-3"></i></a>
                                    </div>
                                    <div class="col-lg-5 d-none d-lg-flex animated zoomIn">
                                        <img class="img-fluid" src="Vue/img/carousel-1.png" alt="">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <%} else %>
                <div class="carousel-item">
                        <img class="w-100" src="./Vue/img/carousel-bg-1.jpg" alt="Image">
                        <div class="carousel-caption d-flex align-items-center">
                            <div class="container">
                                <div class="row align-items-center justify-content-center justify-content-lg-start">
                                    <div class="col-10 col-lg-7 text-center text-lg-start">
                                        <h6 class="text-white text-uppercase mb-3 animated slideInDown">// RAUDI //
                                        </h6>
                                        <h1 class="display-3 text-white mb-4 pb-3 animated slideInDown"><%= element.nom %></h1>
                                        <a href="modele/<%=element.id%>" class="btn btn-primary py-3 px-5 animated slideInDown">Voir plus<i
                                                class="fa fa-arrow-right ms-3"></i></a>
                                    </div>
                                    <div class="col-lg-5 d-none d-lg-flex animated zoomIn">
                                        <img class="img-fluid" src="../Vue/img/carousel-1.png" alt="">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <%}); %>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#header-carousel" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#header-carousel" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
    <!-- Carousel End -->


    <!-- Service Start -->
    <div class="container-xxl py-5">
        <div class="container">
            <div class="row g-4">
                <% modeles.forEach(modele => {%>
                <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="d-flex py-5 px-4">
                        <i class="fa fa-car fa-3x text-primary flex-shrink-0"></i>
                        <div class="ps-4">
                            <h4 class="mb-3"><%= modele.nom %></h4>
                            <p class="descri"><%= modele.Description  %></p>
                            <a class="text-secondary border-bottom" href="modele/<%=modele.id%>">Voir plus</a>         
                        </div>
                    </div>
                </div>
                <%}); %>
            </div>
        </div>
    </div>
    <!-- Service End -->


    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
