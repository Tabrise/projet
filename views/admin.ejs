<body>
    <!-- Section du tableau des achats -->
    <div class="container mt-5">
        <h2>Liste des Achats</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Nom du Client</th>
                    <th scope="col">Modèle de Voiture</th>
                    <th scope="col">Prix</th>
                    <th scope="col">Date d'Achat</th>
                </tr>
            </thead>
            <tbody>
                    <% achats.forEach(achat => {%>
                        <tr>
                            <th scope="row"><%= achat.id %></th>
                            <td>John Doe</td>
                            <td>Modèle R1</td>
                            <td><%= achat.prix %></td>
                            <td><%= achat.dateAchat %></td>
                        </tr>
                    <% }); %>
                <!-- Ajoutez plus de lignes au besoin pour chaque achat -->
            </tbody>
        </table>
    </div>

    <!-- Section du tableau des gains du mois -->
    <div class="container mt-5">
        <h2>Gains du Mois</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Mois</th>
                    <th scope="col">Total des Gains</th>
                </tr>
            </thead>
            <tbody>
                <% mois.forEach(el =>{%>
                    <tr>
                        <td><%= el.dataValues.Mois %></td>
                        <td><%= el.dataValues.SommeTotalAchat %>€</td>
                    </tr>
                <%})%>
              
                <!-- Ajoutez plus de lignes au besoin pour chaque mois -->
            </tbody>
        </table>
    </div>


    <!-- Scripts JavaScript et Bootstrap JS -->
    <script src="js/admin.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.7.0/dist/js/bootstrap.min.js"></script>
    <!-- Ajoutez d'autres scripts au besoin -->

</body>

</html>